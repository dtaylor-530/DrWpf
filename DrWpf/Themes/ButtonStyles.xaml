<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!--<Style x:Key="{x:Type Button}" />-->

  <Style x:Key="GelWithAnimatedStroke" TargetType="{x:Type Button}">
    <Setter Property="ClipToBounds" Value="False" />
    <Setter Property="Margin" Value="2" />
    <Setter Property="Padding" Value="10,2,10,4" />
    <Setter Property="Background">
      <Setter.Value>
        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
          <GradientBrush.GradientStops>
            <GradientStopCollection>
              <GradientStop Color="#66666666" Offset="0" />
              <GradientStop Color="transparent" Offset="1" />
            </GradientStopCollection>
          </GradientBrush.GradientStops>
        </LinearGradientBrush>
      </Setter.Value>
    </Setter>
    <Setter Property="FontSize" Value="8.5pt" />
    <Setter Property="FontFamily" Value="Arial" />
    <Setter Property="FontWeight" Value="Bold" />
    <Setter Property="Foreground" Value="White" />
    <Setter Property="FocusVisualStyle" Value="{x:Null}" />
    <Setter Property="MinHeight" Value="30" />
    <Setter Property="HorizontalContentAlignment" Value="Center" />
    <Setter Property="VerticalContentAlignment" Value="Center" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Button}">
          <Grid x:Name="ButtonGrid" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
            <Rectangle x:Name="GelBackground" RadiusX="9" RadiusY="9" Fill="{TemplateBinding Background}" StrokeThickness="2">
              <Rectangle.Stroke>
                <LinearGradientBrush StartPoint="0,0" EndPoint=".5,0">
                  <LinearGradientBrush.RelativeTransform>
                    <TransformGroup>
                      <TransformGroup.Children>
                        <TransformCollection>
                          <RotateTransform Angle="0" />
                          <TranslateTransform X="0.5" Y="0.5" />
                        </TransformCollection>
                      </TransformGroup.Children>
                    </TransformGroup>
                  </LinearGradientBrush.RelativeTransform>
                  <LinearGradientBrush.GradientStops>
                    <GradientStop Color="Transparent" Offset="0" />
                    <GradientStop Color="#ffffff" Offset="1" />
                  </LinearGradientBrush.GradientStops>
                </LinearGradientBrush>
              </Rectangle.Stroke>
            </Rectangle>
            <Rectangle x:Name="OutlineMask" RadiusX="10" RadiusY="10" Fill="Transparent" StrokeThickness="1" Stroke="#bbffffff" />
            <Rectangle x:Name="Shine" RadiusY="6" RadiusX="6" VerticalAlignment="Top" Stroke="Transparent" Margin="7,3,7,0" Height="7">
              <Rectangle.Fill>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                  <GradientBrush.GradientStops>
                    <GradientStopCollection>
                      <GradientStop Color="#ccffffff" Offset="0" />
                      <GradientStop Color="Transparent" Offset="1" />
                    </GradientStopCollection>
                  </GradientBrush.GradientStops>
                </LinearGradientBrush>
              </Rectangle.Fill>
            </Rectangle>
            <ContentPresenter VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                TextBlock.Foreground="{TemplateBinding Foreground}" TextBlock.FontSize="{TemplateBinding FontSize}"
                TextBlock.FontWeight="{TemplateBinding FontWeight}" Margin="{TemplateBinding Padding}" />
            <Rectangle x:Name="ContentMask" RadiusX="10" RadiusY="10" Fill="Transparent" StrokeThickness="0" />
          </Grid>
          <ControlTemplate.Resources>
          </ControlTemplate.Resources>
          <ControlTemplate.Triggers>
            <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="ButtonGrid">
              <EventTrigger.Actions>
                <BeginStoryboard Name="AnimatedStroke">
                  <BeginStoryboard.Storyboard>
                    <Storyboard TargetName="GelBackground" TargetProperty="(Rectangle.Stroke).(LinearGradientBrush.RelativeTransform).(TransformGroup.Children)[0].(RotateTransform.Angle)">
                      <DoubleAnimation From="0" To="360" Duration="00:00:1" RepeatBehavior="Forever" />
                    </Storyboard>
                  </BeginStoryboard.Storyboard>
                </BeginStoryboard>
              </EventTrigger.Actions>
            </EventTrigger>
            <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="ButtonGrid">
              <EventTrigger.Actions>
                <StopStoryboard BeginStoryboardName="AnimatedStroke" />
              </EventTrigger.Actions>
            </EventTrigger>
            <Trigger Property="IsMouseOver" Value="True">
              <Setter TargetName="GelBackground" Property="Fill">
                <Setter.Value>
                  <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                    <GradientBrush.GradientStops>
                      <GradientStopCollection>
                        <GradientStop Color="Navy" Offset="0" />
                        <GradientStop Color="#99ccff" Offset="1" />
                      </GradientStopCollection>
                    </GradientBrush.GradientStops>
                  </LinearGradientBrush>
                </Setter.Value>
              </Setter>
              <Setter TargetName="OutlineMask" Property="Stroke" Value="Transparent" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
              <Setter TargetName="OutlineMask" Property="Stroke" Value="#33000080" />
              <Setter TargetName="ContentMask" Property="Fill">
                <Setter.Value>
                  <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                    <GradientBrush.GradientStops>
                      <GradientStopCollection>
                        <GradientStop Color="#33000000" Offset="0" />
                        <GradientStop Color="transparent" Offset="1" />
                      </GradientStopCollection>
                    </GradientBrush.GradientStops>
                  </LinearGradientBrush>
                </Setter.Value>
              </Setter>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
              <Setter TargetName="GelBackground" Property="Fill" Value="#777777" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Style.Triggers>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Opacity" Value=".3" />
      </Trigger>
    </Style.Triggers>
  </Style>

  <Style x:Key="ExpandingGelButton" TargetType="{x:Type Button}" BasedOn="{x:Null}">
    <Setter Property="Background" Value="RoyalBlue"/>
    <Setter Property="Padding" Value="10" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Button}">
          <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,0,0" x:Name="Grid" Opacity="1" Cursor="Hand" RenderTransformOrigin="0.5,0.5">
            <Grid.ColumnDefinitions>
              <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
              <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.RenderTransform>
              <TransformGroup>
                <ScaleTransform x:Name="LayoutScaleTransform" ScaleX="1" ScaleY="1"/>
              </TransformGroup>
            </Grid.RenderTransform>
            <Ellipse Stroke="#FF000000" Fill="{TemplateBinding Background}" StrokeThickness="2" Margin="0,0,3,3" x:Name="Ellipse"/>
            <Viewbox Margin="5,5,8,8">
              <ContentPresenter Margin="{TemplateBinding Padding}" TextBlock.Foreground="#aaffffff" TextBlock.FontWeight="Bold" TextBlock.FontFamily="Calibri" TextBlock.FontSize="40pt" HorizontalAlignment="center" VerticalAlignment="center" />
            </Viewbox>
            <Ellipse Margin="5,4,8,15" x:Name="ShineEllipse">
              <Ellipse.Fill>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                  <GradientStop Color="sc#0, 1, 1, 1" Offset="1"/>
                  <GradientStop Color="sc#0.8, 1, 1, 1" Offset="0"/>
                </LinearGradientBrush>
              </Ellipse.Fill>
            </Ellipse>
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Property="IsFocused" Value="True">
              <Setter TargetName="Grid" Property="Grid.ZIndex" Value="3" />
              <Setter Property="RenderTransform" TargetName="Grid">
                <Setter.Value>
                  <TransformGroup>
                    <ScaleTransform ScaleX="1.25" ScaleY="1.25"/>
                  </TransformGroup>
                </Setter.Value>
              </Setter>
            </Trigger>
            <Trigger Property="IsDefaulted" Value="True"/>
            <Trigger Property="IsMouseOver" Value="True">
              <Setter TargetName="Grid" Property="Grid.ZIndex" Value="1" />
              <Setter TargetName="Grid" Property="Opacity" Value="1" />
              <Trigger.EnterActions>
                <BeginStoryboard>
                  <Storyboard>
                    <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(ScaleTransform.ScaleY)" Storyboard.TargetName="LayoutScaleTransform" DecelerationRatio="1">
                      <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5" Value="1.25" KeyTime="00:00:00.15"/>
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(ScaleTransform.ScaleX)" Storyboard.TargetName="LayoutScaleTransform" DecelerationRatio="1">
                      <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5" Value="1.25" KeyTime="00:00:00.15"/>
                    </DoubleAnimationUsingKeyFrames>
                  </Storyboard>
                </BeginStoryboard>
              </Trigger.EnterActions>
              <Trigger.ExitActions>
                <BeginStoryboard>
                  <Storyboard>
                    <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(ScaleTransform.ScaleY)" Storyboard.TargetName="LayoutScaleTransform" AccelerationRatio="1">
                      <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5" Value="1" KeyTime="00:00:00.15"/>
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(ScaleTransform.ScaleX)" Storyboard.TargetName="LayoutScaleTransform" AccelerationRatio="1">
                      <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5" Value="1" KeyTime="00:00:00.15"/>
                    </DoubleAnimationUsingKeyFrames>
                  </Storyboard>
                </BeginStoryboard>
              </Trigger.ExitActions>
            </Trigger>
            <Trigger Property="IsPressed" Value="True"/>
            <Trigger Property="IsEnabled" Value="False"/>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

